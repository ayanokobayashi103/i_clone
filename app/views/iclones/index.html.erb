<h1>Iclones#index</h1>
<p>Find me in app/views/iclones/index.html.erb</p>
<p><%= notice %></p>
<table>
  <tr>
    <th>タイトル</th>
    <th>内容</th>
  </tr>
  <% @iclones.each do |iclone| %>
  <tr>
    <td><%= iclone.content %></td>
    <td><%= link_to '詳細を確認する', iclone_path(iclone.id) %></td>
    <td><%= link_to "ブログを編集する", edit_iclone_path(iclone.id) %></td>
    <td><%= link_to 'ブログを削除する', iclone_path(iclone.id), method: :delete, data: { confirm: '本当に削除していいですか？' }  %></td>
  </tr>
  <% end %>
</table>
<%= link_to '新規作成', new_iclone_path %>
